{
  "name": "AuthToken",
  "base": "PersistedModel",
  "idInjection": false,
  "options": {
    "validateUpsert": true
  },
  "mixins": {},
  "protected": [
    "type"
  ],
  "properties": {
    "id": false,
    "token": {
      "type": "string",
      "required": true
    },
    "type": {
      "type": "string",
      "required": true,
      "default": "password"
    },
    "ttl": {
      "type": "number",
      "description": "time to live in seconds (2 weeks by default)",
      "ttl": true,
      "default": 1209600
    },
    "updatedAt": {
      "type": "date"
    }
  },
  "validations": [],
  "relations": {},
  "acls": [],
  "methods": {}
}
